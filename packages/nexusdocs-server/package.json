{
  "name": "nexusdocs-server",
  "version": "0.3.2",
  "description": "NexusHubs server, a storage engine proxy for multi provider",
  "main": "lib/index.js",
  "scripts": {
    "tsc": "tsc -p ./src",
    "tscw": "tsc -p ./src -w",
    "clean": "rm -rf lib/*",
    "start": "node lib/index start",
    "build": "babel --out-dir lib src --source-maps --extensions \".ts\"",
    "watch": "babel --out-dir lib src --source-maps --extensions \".ts\" --watch",
    "nodemon": "nodemon --delay 1000ms -i src lib/app start",
    "debug": "nodemon --inspect --delay 1000ms -i src lib/app start",
    "dev": "yarn watch & yarn nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
