{
  "name": "nexusdocs-client",
  "version": "0.4.5",
  "description": "Client SDK for nexusdocs-server",
  "main": "lib/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "tsc": "tsc -p ./src",
    "tscw": "tsc -p ./src -w",
    "clean": "rm -rf lib/*",
    "start": "node lib/index start",
    "build": "yarn tsc",
    "watch": "yarn tscw",
    "nodemon": "nodemon --delay 1000ms -i src lib/index start",
    "debug": "nodemon --inspect --delay 1000ms -i src lib/index start",
    "dev": "yarn tscw & yarn nodemon",
    "test": "npm run build && node_modules/.bin/mocha"
  },
  "repository": "https://github.com/nexushubs/nexusdocs/tree/master/packages/nexusdocs-client",
  "keywords": [
    "storage",
    "file",
    "system"
  ],
  "author": "alphakevin",
  "license": "MIT",
  "devDependencies": {
    "@types/content-disposition": "^0.5.2",
    "@types/crypto-js": "^3.1.43",
    "@types/decamelize": "^3.0.3",
    "@types/is-stream": "^1.1.0",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash": "^4.14.123",
    "@types/mime-types": "^2.1.0",
    "@types/qs": "^6.5.3",
    "@types/request": "^2.48.1",
    "@types/url-parse": "^1.4.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "jsdoc-to-markdown": "^3.0.4",
    "mocha": "^6.1.4",
    "nock": "^9.2.5",
    "source-map-support": "^0.5.6",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "content-disposition": "^0.5.2",
    "crypto-js": "^3.1.9-1",
    "decamelize": "^3.2.0",
    "is-stream": "^1.1.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.10",
    "mime-types": "^2.1.18",
    "qs": "^6.5.2",
    "request": "^2.87.0",
    "url-parse": "^1.4.4"
  }
}
