{
  "name": "nexusdocs-client",
  "version": "0.5.0-alpha.1",
  "description": "Client SDK for nexusdocs-server",
  "main": "lib/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib/*",
    "start": "node lib/index start",
    "build": "tsc -p ./src",
    "watch": "tsc -p ./src -w",
    "nodemon": "nodemon --delay 1000ms -i src lib/index start",
    "debug": "nodemon --inspect --delay 1000ms -i src lib/index start",
    "dev": "yarn watch & yarn nodemon",
    "test": "mocha",
    "build:doc": "jsdoc2md --files ./src/*.ts --configure ./jsdoc2md.json > ./docs/API.md",
    "prepublishOnly": "yarn build && yarn test && yarn build:doc"
  },
  "repository": "https://github.com/nexushubs/nexusdocs/tree/master/packages/nexusdocs-client",
  "keywords": [
    "storage",
    "file",
    "system"
  ],
  "author": "alphakevin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/content-disposition": "^0.5.2",
    "@types/content-type": "^1.1.3",
    "@types/crypto-js": "^3.1.43",
    "@types/decamelize": "^3.0.3",
    "@types/is-stream": "^1.1.0",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash": "^4.14.136",
    "@types/mime-types": "^2.1.0",
    "@types/node-fetch": "^2.5.0",
    "@types/qs": "^6.5.3",
    "@types/source-map-support": "^0.5.0",
    "@types/url-parse": "^1.4.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "documentation": "^12.1.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "nock": "^9.2.5",
    "typescript": "^3.5.3",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "content-disposition": "^0.5.2",
    "content-type": "^1.0.4",
    "crypto-js": "^3.1.9-1",
    "decamelize": "^3.2.0",
    "is-stream": "^1.1.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.14",
    "mime-types": "^2.1.18",
    "node-fetch": "^2.6.0",
    "qs": "^6.5.2",
    "source-map-support": "^0.5.13",
    "url-parse": "^1.4.4"
  }
}
